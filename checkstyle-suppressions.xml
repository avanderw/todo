<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="JavadocPackage"/>
    <suppress checks="JavadocType"/>
    <suppress checks="JavadocVariable"/>
    <suppress checks="JavadocMethod"/>
    <suppress checks="OperatorWrap"/>
    <suppress checks="LineLength"/>
    <suppress checks="AvoidStarImport"/>
    <suppress checks="HiddenField"/>
    <suppress checks="NewlineAtEndOfFile"/>
    <suppress checks="EmptyBlock" files="Interpolation"/>
    <suppress checks="MagicNumber"
              files="AnsiColor|Interpolation|.*Module|ColorConverter|.*Generator|PercentageRenderer|.*Palette"/>
    <suppress checks="DesignForExtension" files="TodoItem|.*Palette|.*Theme|.*Module|AnsiColor"/>
</suppressions>