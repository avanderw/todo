<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="JavadocPackage"/>
    <suppress checks="JavadocType"/>
    <suppress checks="JavadocVariable"/>
    <suppress checks="JavadocMethod"/>
    <suppress checks="OperatorWrap"/>
    <suppress checks="LineLength"/>
    <suppress checks="HiddenField"/>
    <suppress checks="NewlineAtEndOfFile"/>
    <suppress checks="MagicNumber"/>
    <suppress checks="EmptyBlock" files="Interpolation"/>
    <suppress checks="DesignForExtension"/>
    <suppress checks="InnerAssignment" files="RelativeDate"/>
    <suppress checks="WhitespaceAround" files="DateFilterMixin"/>
    <suppress checks="VisibilityModifier" files=".*Mixin|.*Github.*"/>
    <suppress checks="MemberName" files=".*Github.*"/>
    <suppress checks=".*" files="[/\\]target[/\\]"/>
</suppressions>